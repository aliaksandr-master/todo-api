RewriteEngine On
Options +FollowSymlinks

RewriteRule .* index.php [L,QSA]


# custom 404 page
ErrorDocument 404 '{"status":404,"error":{"system":[{"404":"not_found"}]}}'
ErrorDocument 403 '{"status":403,"error":{"system":["403":"forbidden"]}}'
ErrorDocument 401 '{"status":401,"error":{"system":["401":"unauthrized"]}}'