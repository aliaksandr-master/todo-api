<li class="todo-lists-li{{#if shared}} -shared{{/if}}" data-listId="{{listId}}">
    <span class="glyphicon glyphicon-resize-vertical todo-lists-li-move"></span>
    <button class="btn todo-lists-li-remove btn-default">
        <span class="glyphicon glyphicon-trash"></span>
    </button>
    <a href="{{url 'todo-list-share' listId}}" class="btn btn-default todo-lists-li-share">
        <span class="glyphicon glyphicon-share-alt"></span>
    </a>
    <a href="{{url 'todo-list' listId}}" class="todo-lists-li-link">
        {{title}}
    </a>
</li>